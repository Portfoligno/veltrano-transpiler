fun calculateSum(firstNumber: I64, secondNumber: I64): I64 {
    val totalResult: I64 = firstNumber + secondNumber
    return totalResult
}

fun main() {
    val myFirstValue: I64 = 10
    val mySecondValue: I64 = 20
    val finalResult: I64 = calculateSum(myFirstValue, mySecondValue)
    println("{}", finalResult)
}
